function generateRandomChar(length)
local chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
local result = ""
for _ = 1, length do
    local randomIndex = math.random(1, #chars)
    result = result .. string.sub(chars, randomIndex, randomIndex)
end
return result
end

