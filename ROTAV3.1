function tablelength(T)
local count = 0
for _ in pairs(T) do
    count = count + 1
end
return count
end

function split(inputstr, sep)
if sep == nil then
    sep = "%s"
end
local t = {}
for str in string.gmatch(inputstr, "([^" .. sep .. "]+)") do
    table.insert(t, str)
end
return t
end

function read(filename)
local ListWorlds = {}
local file = io.open(filename, "r")
if file then
    for line in file:lines() do
        table.insert(ListWorlds, line)
    end
    file:close()
else
    print("[Cema Store] Update: File ".. filename .." Not Found")
    bot:stopScript()
end

return ListWorlds
end

function ReadPnb(CemaScript)
local fileName = CemaScript
local file = io.open(fileName, "r")
if file then
    local lines = {}
    for line in file:lines() do
        table.insert(lines, line)
    end
    file:close()
    ReadFile = lines[1]
    data = split(lines[1], ':')
    if tablelength(data) == 2 then
        Xcell1 = data[1]
        Xcell2 = data[2]
    end
    table.remove(lines,1)
    file = io.open(fileName, "w")
    if file then
        for _,line in ipairs(lines) do
            file:write(line .. "\n")
        end
        file:write(ReadFile)
        file:close()
    end
end
return Xcell1,Xcell2
end

