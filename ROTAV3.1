function CariTileIni(tbl, name)
for i, v in ipairs(tbl) do
    if v == name then
        return i
    end
end
return nil
end

function CariPerbedaanIni(tbl)
local result = {}
for i, v in ipairs(tbl) do
    if i % 2 == 1 then
        table.insert(result, v)
    end
end
return result
end

