                    end
                end
            end

        end
    end

end
end

function BisaPlant(x, y)
local tempTile = getTile(x, y+1)
if not tempTile.fg then
    return false
end
local collision = getInfo(tempTile.fg).collision_type
return tempTile and ( collision == 1 or collision == 2 or collision == 4)
end

