function PosisiDrop(x, y, num)
local count = 0
local stack = 0
for _,obj in pairs(bot:getWorld():getObjects()) do
    if RandomPosition(obj.x / 32) == x and math.floor(obj.y / 32) == y then
        count = count + obj.count
        stack = stack + 1
    end
end
if stack < 20 and count <= (4000 - num) then
    return true
end
return false
end

